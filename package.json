{
  "name": "nodekell",
  "version": "1.8.3",
  "description": "async functional library",
  "repository": {
    "type": "git",
    "url": "https://github.com/rudty/nodekell"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.js test/**/*.spec.ts && dtslint",
    "dtslint": "dtslint",
    "build" : "node build.js && rollup -c ./rollup.config.js"
  },
  "keywords": [
    "haskell",
    "prelude",
    "async",
    "function",
    "functional",
    "stream",
    "iterator",
    "functor",
    "curry",
    "map",
    "filter",
    "range",
    "reduce",
    "list",
    "fold",
    "collect",
    "linq",
    "generator",
    "lazy",
    "pmap",
    "parallel",
    "pfilter",
    "lisp",
    "parallel"
  ],
  "author": "rudtyz",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.3"
  },
  "bugs": "https://github.com/rudty/nodekell/issues",
  "dependencies": {
    "install": "^0.12.2",
    "npm": "^6.9.0",
    "rollup": "^1.14.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-node-resolve": "^5.0.1"
  }
}
