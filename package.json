{
  "name": "nodekell",
  "version": "2.7.3",
  "description": "async functional library",
  "repository": {
    "type": "git",
    "url": "https://github.com/rudty/nodekell"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build && mocha -r ts-node/register test/**/*.js test/**/*.spec.ts && npm run local_dtslint",
    "dtslint": "dtslint",
    "local_dtslint": "dtslint --localTs ./node_modules/typescript/lib",
    "build": "rollup -c ./rollup.config.js",
    "watch": "rollup -w -c ./rollup.config.js",
    "prepublishOnly": "npm run build",
    "mocha": "mocha"
  },
  "keywords": [
    "haskell",
    "prelude",
    "async",
    "function",
    "functional",
    "stream",
    "iterator",
    "functor",
    "curry",
    "map",
    "filter",
    "range",
    "reduce",
    "list",
    "fold",
    "collect",
    "linq",
    "generator",
    "lazy",
    "pmap",
    "parallel",
    "pfilter",
    "lisp",
    "parallel"
  ],
  "author": "rudtyz",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.3"
  },
  "bugs": "https://github.com/rudty/nodekell/issues",
  "devDependencies": {
    "dtslint": "^0.8.0",
    "mocha": "^6.2.2",
    "rollup": "^1.25.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-terser": "^5.1.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  }
}
