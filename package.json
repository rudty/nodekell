{
  "name": "nodekell",
  "version": "1.12.2",
  "description": "async functional library",
  "repository": {
    "type": "git",
    "url": "https://github.com/rudty/nodekell"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build && mocha -r ts-node/register test/**/*.js test/**/*.spec.ts && dtslint",
    "dtslint": "dtslint",
    "build": "rollup -c ./rollup.config.js",
    "watch": "rollup -w -c ./rollup.config.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "haskell",
    "prelude",
    "async",
    "function",
    "functional",
    "stream",
    "iterator",
    "functor",
    "curry",
    "map",
    "filter",
    "range",
    "reduce",
    "list",
    "fold",
    "collect",
    "linq",
    "generator",
    "lazy",
    "pmap",
    "parallel",
    "pfilter",
    "lisp",
    "parallel"
  ],
  "author": "rudtyz",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.3"
  },
  "bugs": "https://github.com/rudty/nodekell/issues",
  "devDependencies": {
    "rollup": "^1.14.2"
  }
}
